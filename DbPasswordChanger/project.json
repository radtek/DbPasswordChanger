  "publishOptions": {
    "includeFiles": [ "config.ini", "nlog.config" ]
  },
  "dependencies": {
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.0.0"
    },
    "SapientGuardian.MySql.Data": "6.9.812",
    "Dapper": "1.50.2",
    "Microsoft.Extensions.Configuration.Ini": "1.0.0",
    "Microsoft.Extensions.Configuration.Binder": "1.0.0",
    "System.Text.Encoding.CodePages": "*",
    "NLog": "4.4.0-betaV15"
  },

  "frameworks": {
    "netcoreapp1.0": {
      "imports": "portable-net45+win8+dnxcore50",
      "dependencies": {},
      "buildOptions": {
        "define": [
          "RELEASE",
          "NET_CORE",
          "Publish"
        ]
      }
    }
